; int.franzen.fr
$TTL 120	; 2 minutes
int.franzen.fr.			IN SOA	dns.int.franzen.fr. postmaster.franzen.fr. (
				2020111110 ; SERIAL
				3600       ; REFRESH (1 hour)
				15M        ; RETRY (15 minutes)
				1W    	   ; EXPIRE (1 week)
				600        ; minimum / Negative cache TTL (5 minutes)
				)

;; NS Records - Authoritative DNS
int.franzen.fr.			IN NS	dns.int.franzen.fr.

